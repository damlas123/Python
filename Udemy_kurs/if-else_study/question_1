print("WELCOME INDEX CALCULATOR")
weight=None
x=None
height=None
weight=float(input("enter your weight:"))
height=float(input("enter your height:"))
x=weight/height**2
if x<=18.5:
    print("Very weak")
elif x>18 and x<=25:
    print("normal")
elif x>25 and x<=30:
    print("very fat")
else:
    print("obezite")
